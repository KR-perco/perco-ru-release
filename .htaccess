<IfModule mod_headers.c>
    Header set Access-Control-Allow-Origin "*"
</IfModule>

<IfModule mod_php5.c>
  php_flag allow_call_time_pass_reference 1
  php_flag session.use_trans_sid off

  #php_value display_errors 1
</IfModule>

<IfModule mod_rewrite.c>
  Options +FollowSymLinks
  RewriteEngine On

	RewriteCond %{HTTP:X-HTTPS} !1
	RewriteCond %{HTTP_HOST} ^www.perco.ru$
	RewriteRule ^(.*) https://www.perco.ru/$1 [R,L]

	#RewriteCond %{HTTP:X-HTTPS} !1
	#RewriteRule ^client/?(.*) https://www.perco.ru/client/$1 [R,L]

	#RewriteCond %{HTTP:X-HTTPS} !1
	#RewriteRule ^bitrix/admin/?(.*) https://www.perco.ru/bitrix/admin/$1 [R,L]

  RewriteCond %{REQUEST_FILENAME} ^(.*)/DD.MM.YYYY$
  RewriteRule ^(.*)$ rewriteurl.php?route=$1 [L,QSA]
  RewriteRule ^images/subscribe/pixel/$ /images/subscribe/pixel/pixel.gif [R=301,L]


